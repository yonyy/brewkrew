{
  "name": "brewkrew",
  "version": "1.0.0",
  "description": "Just a group of friends conquering San Diego one brewery at a time",
  "main": "index.html",
  "repository": "https://github.com/yonyy/brewkrew",
  "author": "Yonatan Perez",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "eslint": "^4.19.1",
    "express": "^4.16.3",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-modal": "^3.4.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "node-sass": "^4.9.0",
    "parcel-bundler": "^1.9.3"
  },
  "scripts": {
    "start": "rm -rf ./dist && parcel index.html -p 8080 --no-cache",
    "build": "rm -rf ./dist && parcel build index.html --out-dir dist/ --no-source-maps --no-cache",
    "publish": "npm run build && aws s3 sync ./dist s3://brewkrew.io/ --exclude *.html --exclude '*.map' --cache-control 'max-age=31536000, private' && aws s3 sync ./dist s3://brewkrew.io/ --exclude *.js* --cache-control 'no-cache' --delete"
  }
}
